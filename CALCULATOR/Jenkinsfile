pipeline {
agent any
environment {
    JAVA_HOME="C:/Program Files/Java/jdk-17"
  }
stages {
stage('Clean stage') {
steps {
dir("CALCULATOR") {
bat 'mvn clean'
}
}
}

stage('Compile stage') {
steps {
dir("CALCULATOR") {
bat 'mvn compile'
}
}
}

stage('Install stage') {
steps {
dir("CALCULATOR") {
bat 'mvn install'
}
}
}

}
}